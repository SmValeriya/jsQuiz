@import url("../utils/variables");

@mixin tablets() {
  @media (max-width: $tablets) {
    @content ;
  }
}

@mixin phones() {
  @media (max-width: $phones) {
    @content ;
  }
}

@mixin minphones() {
  @media (max-width: $minphones) {
    @content ;
  }
}

@mixin media-custom($width) {
  @media (min-width: $width) {
    @content ;
  }
}
